  <div id="wrapper" class="">

<header class="jobs-header">
  <span id="total_count">
    <%=@questions.count%>
  </span>
  Questions 
    <a href= "/questions/new"><span class="job-tag role">Ask a Question</span></a>      

</header>


<table id="jobs_table">
  <thead>
    <tr class="table-header">
      <th class="table-header-item bulk-edit-header"><input type="checkbox"></th>
      <th class="table-header-item">Question</th>
      <th class="table-header-item">Username</th>
      <th class="table-header-item">Comments</th>
      <th class="table-header-item">Date Updated</th>
    </tr>
  </thead>
  <tbody>
    <%@questions.each do |question|%>
      <tr class="job">

  <td class="job-cell job-name">

    <a title="<%=question.title%>" class="target" href='/questions/<%=question.id%>'>

      <div class="cell-content">
        <div class="job-title">

          <span class="cell-text job-label-name"><%=question.title%></span>


          <div class="job-tags">
            
            

            
            <span class="job-tag role"><%=question.lab%></span>
            
            
            
          </div>
        </div>
      </div>

</a>
  </td>

  <td class="job-cell req-id">
    <div class="cell-content">
      <div class="cell-text txt-light">
        <span class="cell-text txt-light" title="Education - Faculty">
        <%if question.student.nil?%>
            TBD
        <%else%>
            <%= question.student_username%>
        <%end%>
      </span>
      </div>
    </div>
  </td>

  <td class="job-cell job-department">
    <div class="cell-content">
      <span class="cell-text txt-light" title="Education - Faculty">
       <%=question.comments.count%>
      </span>
    </div>
  </td>

  <td class="job-cell job-location">
    <div class="cell-content">
        <span class="cell-text txt-light" title="Remote"><%=question.created_at%></span>
    </div>
  </td>
</tr>
<%end%>
  </tbody>
</table>

