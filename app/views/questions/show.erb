<div class="job-setup-container">
  <div class="content-box content-box-alt">
  <div class="header">
    <h4><%= @question.title%></h4>
    <div class="content-header-right-side">
        <%if @question.student == @current_user%>
          <a class="large-button" href="/questions/<%=@question.id%>/edit">Edit</a>
          <a class="large-button" href="/questions/<%=@question.id%>/delete">Delete</a>
        <%end%>
    </div>
  </div>
  <div class="content">
  <pre style = "display: block;
    font-family: monospace;
    white-space: pre;
    margin: 1em 0;">
    <%=@question.content%>
  </pre>
    <table class="job-setup-table job-info-table">
      <tbody><tr>
        <td width="25%"><%=@question.student_username%></td>
        <td width="25%"><%=@question.created_at%></td>
        <td width="25%"><a href="<%= @question.link%>"><%=@question.lab%></a></td>
      </tr>
    </tbody></table>
 </div>
</div>

<div class="job-setup-container">
  <div class="content-box content-box-alt">
    <div class="header">
        <h4>Comments</h4> <a href= "/comments/new"><span class="job-tag">Answer the Question</span></a>      
        </div>
    <%@question.comments.each do |comment|%>
    <div class = "content">
      <pre style="
    display: block;
    font-family: monospace;
    white-space: pre;
    margin: 1em 0;">
        <%=comment.content%>
     </pre>
    <table class="job-setup-table job-info-table">
      <tbody><tr>
        <td width="25%"><%=comment.student_username%></td>
        <td width="25%"><%=comment.created_at%></td>
        <%if comment.student === @current_user%>
            <a href = "/comments/<%=comment.id%>/edit"<td width="25%">Edit</td></a>
            |
            <a href = "/comments/<%=comment.id%>/delete"<td width="25%">Delete</td></a>
        <%end%>
        


      </tr>
    </tbody></table>
    </div>
<%end%>
